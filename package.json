{
  "name": "simple-google-ads",
  "description": "WordPress plugin to display Google Ad Manager ads.",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/swissspidy/simple-google-ads.git"
  },
  "homepage": "https://github.com/swissspidy/simple-google-ads#readme",
  "bugs": {
    "url": "https://github.com/swissspidy/simple-google-ads/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@wordpress/babel-plugin-import-jsx-pragma": "^1.1.3",
    "@wordpress/babel-preset-default": "^3.0.2",
    "@wordpress/blocks": "^6.3.0",
    "@wordpress/components": "^7.4.0",
    "@wordpress/element": "^2.4.0",
    "@wordpress/i18n": "^3.4.0",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0",
    "postcss-nested": "^4.1.2",
    "raw-loader": "^1.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "@wordpress/default"
    ],
    "plugins": [
      [
        "@wordpress/babel-plugin-import-jsx-pragma",
        {
          "scopeVariable": "createElement",
          "source": "@wordpress/element",
          "isDefault": false
        }
      ]
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env BABEL_ENV=default webpack --watch",
    "makepot": "wp i18n make-pot .",
    "release": "npm run build && npm run makepot"
  }
}
